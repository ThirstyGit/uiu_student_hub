<!DOCTYPE html>
<html lang="en">
<head>
   <meta charset="UTF-8">
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <title>Document</title>
</head>
<body>
   <a href="/">HOME</a>
   <div>
      <% users.forEach( user => { %>
         <h3><%=user.name%></h3>
         <form action="/course/<%=course_id%>/moderate/addmoderate" method="POST">
            <input value="<%=user.user_id%>" name="id" hidden>
            <button>ADD MODERATOR</button>
         </form>
         <form action="/course/<%=course_id%>/moderate/removemoderate" method="POST">
            <input value="<%=user.user_id%>" name="id" hidden>
            <button>REMOVE MODERATOR</button>
         </form>
      <%})%>
   </div>
   
</body>
</html>